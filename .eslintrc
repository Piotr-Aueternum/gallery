{
	"globals": {
    "document": true,
    "window": true,
    "DOMParser": true,
    "fetch": true,
    "XMLHttpRequest": true
	},
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalDecorators": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "react/no-array-index-key": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}